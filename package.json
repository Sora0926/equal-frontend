{
  "name": "equal-frontend",
  "version": "0.0.0-development",
  "private": true,
  "homepage": "https://github.com/",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ageha734/equal-frontend.git"
  },
  "engines": {
    "node": ">=18.16.0",
    "pnpm": ">=8.5.0",
    "npm": "please_use_pnpm_instead",
    "yarn": "please_use_pnpm_instead"
  },
  "packageManager": "pnpm@8.5.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "lint-staged": "lint-staged --config .lintstagedrc",
    "changeset": "changeset",
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "ui:dev": "storybook dev -p 6006",
    "ui:build": "storybook build",
    "lint:ts": "eslint --ignore-unknown --ignore-path .ignorerc --fix './**/*.{ts,tsx}'",
    "lint:md": "markdownlint \"**/*.md\" --fix -c .markdownlintrc --ignore-path=.ignorerc",
    "lint:style": "stylelint --ignore-path .ignorerc --fix './**/*.{css,scss,sass}'",
    "lint:spelling": "cspell lint -c .cspellrc \"./**/*.{md,mdx,ts,tsx,css,scss,sass}\" | node .makejson.js > spellingError.json",
    "format": "eslint-config-prettier './**/*.{md,mdx,ts,tsx,css,scss,sass}'",
    "format:ts": "prettier --ignore-unknown --ignore-path .ignorerc --write './**/*.{ts,tsx}'",
    "format:css": "prettier --ignore-unknown --ignore-path .ignorerc --write './**/*.{css,scss,sass}'",
    "format:md": "prettier --ignore-unknown --ignore-path .ignorerc --write './**/*.{md,mdx}'",
    "check:tsc": "tsc --noEmit",
    "check:style": "stylelint --ignore-path .ignorerc './**/*.{css,scss,sass}'",
    "check:format": "prettier --list-different \"./**/*.{md,mdx,ts,tsx,css,scss,sass}\"",
    "check:spelling": "cspell lint -c .cspellrc -r . \"**/*.{md,mdx,ts,tsx,css,scss,sass}\"",
    "list:browse": "pnpm dlx browserslist",
    "list:licenses": "pnpm dlx license-checker --production --summary",
    "list:spelling": "cspell -c .cspellrc \"**/*.{md,mdx,ts,tsx,css,scss,sass}\" --unique --wordsOnly --no-progress --no-summary",
    "test:ui": "playwright test app/_components/.*/*.spec.tsx?$",
    "test:app": "jest --watch",
    "test:e2e": "playwright test",
    "test:mocha": "mocha --config .mocharc test",
    "test:coverage": "tsc && nyc mocha --recursive --timeout 10000",
    "clean": "rimraf node_modules && rimraf .pnpm-store && rimraf .next && rimraf pnpm-lock.yaml && rimraf coverage",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "@apollo/client": "3.7.14",
    "@apollo/react-hooks": "4.0.0",
    "@heroicons/react": "2.0.18",
    "@mdx-js/loader": "2.3.0",
    "@mdx-js/react": "2.3.0",
    "@next-safe/middleware": "0.10.0",
    "@next/mdx": "13.4.3",
    "@t3-oss/env-nextjs": "0.3.1",
    "clsx": "1.2.1",
    "cross-fetch": "3.1.6",
    "date-fns": "2.30.0",
    "dinero.js": "1.9.1",
    "graphql": "16.6.0",
    "ms": "2.1.3",
    "next": "13.4.2",
    "next-compose-plugins": "2.2.1",
    "next-i18next": "13.2.2",
    "preact": "^10.14.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.0.0-rc.1",
    "use-count-up": "3.0.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.1",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@cspell/dict-cryptocurrencies": "3.0.1",
    "@playwright/test": "1.34.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.2",
    "@semantic-release/npm": "10.0.3",
    "@semantic-release/release-notes-generator": "11.0.2",
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/blocks": "^7.0.12",
    "@storybook/nextjs": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@svgr/webpack": "8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "29.5.1",
    "@types/node": "20.2.1",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/parser": "^5.59.6",
    "commitizen": "4.3.0",
    "cspell": "^6.31.1",
    "css-loader": "6.7.4",
    "cz-cli": "1.0.0",
    "cz-customizable": "7.0.0",
    "eslint": "8.40.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "markdownlint-cli": "0.34.0",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "postcss-syntax": "0.36.2",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "rimraf": "5.0.1",
    "sass": "1.62.1",
    "sass-loader": "13.2.2",
    "semantic-release": "21.0.2",
    "storybook": "^7.0.12",
    "style-loader": "3.3.3",
    "stylelint": "^15.6.2",
    "stylelint-a11y": "1.2.3",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "4.0.0",
    "stylelint-config-recommended": "12.0.0",
    "stylelint-config-standard": "33.0.0",
    "stylelint-csstree-validator": "2.1.0",
    "stylelint-declaration-block-no-ignored-properties": "2.7.0",
    "stylelint-no-unsupported-browser-features": "6.1.0",
    "stylelint-prettier": "3.0.0",
    "stylelint-scss": "5.0.0",
    "stylelint-value-no-unknown-custom-properties": "4.0.0",
    "ts-jest": "29.1.0",
    "ts-migrate": "0.1.35",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "windicss": "^3.5.6",
    "windicss-webpack-plugin": "^1.7.8"
  }
}
